{
  "name": "machinejs",
  "version": "0.7.0",
  "description": "Automated machine learning structure. Ensembles random forests, XGBoost, neural networks, AdaBoost, KNN, SVM together, handling data formatting ensembling, and running predictions for you. Feel free to tweak the settings if you want a lot of control, or just start it up and let it run if you're looking for a push-button MVP solution.",
  "main": "ppLib.js",
  "scripts": {
    "test": "npm run test:regression",
    "test:regression": "mocha test/regression/test.js",
    "test:categorical": "mocha test/categorical/test.js",
    "dev": "node ppLib.js data/rossman/rossShortTrainDev.csv --kagglePredict data/rossman/test.csv --join data/rossman/store.csv --alreadyFormatted --dev",
    "devEnsemble": "node ppLib.js data/rossman/rossShortTrainDev.csv --kagglePredict data/rossman/test.csv --join data/rossman/store.csv --alreadyFormatted --devEnsemble",
    "train:rossman": "node ppLib.js data/rossman/tran_filled_gap.csv --kagglePredict data/rossman/test.csv --join store.csv --alreadyFormatted",
    "train:numerai": "node ppLib.js data/numerai/numerai_training_data.csv --kagglePredict data/numerai/numerai_test_data.csv --binaryOutput --alreadyFormatted"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/ClimbsRocks/machinejs.git"
  },
